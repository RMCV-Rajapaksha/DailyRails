{
  "expo": {
    "name": "DailyRails",
    "slug": "dailyrails",
    "scheme": "dailyrails",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "https://maps.googleapis.com/maps/api/geocode/json?address=1600+Amphitheatre+Parkway,+Mountain+View,+CA&key=AIzaSyAOVYRIgupAurZup5y1PRh8Ismb1A3lLao"
        }
      },
      "package": "com.virangaweerabandara.dailyrails"
    },
    "plugins": [
      "expo-router",
      "expo-location"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8dedb87a-0ab6-4b56-b64a-183689972830"
      }
    }
  }
}
